<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.doreou.mapper.OrderMapper">
    <insert id="sale" parameterType="cn.doreou.model.Goods">
        INSERT into t_goods (owner_id,goods_title,introduce,pri_cost,expt_price,status,number,subject,cover,type) values (#{owner_id},#{goods_title},#{introduce},#{pri_cost},#{expt_price},#{status},#{number},#{subject},#{cover},#{type})
    </insert>

    <insert id="buy" parameterType="cn.doreou.model.Goods">
        INSERT into t_goods (goods_title,introduce,expt_price,status,number,subject,type) values (#{goods_title},#{introduce},#{expt_price},#{status},#{number},#{subject},#{type})
    </insert>

</mapper>